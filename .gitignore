# Ignore bundler config.
teste_app/.bundle

# Ignore the default SQLite database.
teste_app/db/*.sqlite3
teste_app/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
teste_app/log/*
teste_app/tmp/*
teste_app!/log/.keep
teste_app!/tmp/.keep

teste_app/node_modules
teste_app/yarn-error.log
teste_app/config/secrets.yml

teste_app/.byebug_history
